<decoders>
    <decoder name="ssh">
        <program_name>sshd</program_name>
        <regex>^.*Failed password  .* from (\S+)</regex>
        <field name="src_ip">(\S+)</field>
    </decoder>
    <decoder name="usb-device-detection">
     <program_name>kernel</program_name>
     <regex>\[.*\]\s*(sd\w+)</regex>
     <field name="device_name">(\S+)</field>
    </decoder>

</decoders>
